<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xueyu.cardphoto" >
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="com.xueyu.base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectFontActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">

        </activity>
        <activity android:name=".PhotoCategoryActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">

        </activity>
        <activity android:name=".PhotoListActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">

        </activity>
        <activity android:name=".TextCategoryActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">

        </activity>
        <activity android:name=".TextListActivity"
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation">

        </activity>
    </application>

</manifest>
